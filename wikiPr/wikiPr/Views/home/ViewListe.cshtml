
@using wikiPr.Models


<table class="table">
    @foreach (Article a in ViewBag.lesArticles) {
        string letitre = a.Titre;

        <tr>
            <td>
                @Html.ActionLink(letitre, "afficher", "home", new { Titre = letitre }, null)
            </td>
            <td>
                @a.DateModification
            </td>
        </tr>
    }
</table>

